citHeader("To cite networkformat in publications use:")

bibentry(
  bibtype  = "Manual",
  title    = "networkformat: Convert Machine Learning Tree Models to Network Edgelists",
  author   = c(
    person("John", "Doe"),
    person("Jane", "Smith")
  ),
  year     = "2026",
  note     = "R package version 0.0.0.9000",
  url      = "https://github.com/yourusername/networkformat",
  textVersion = paste(
    "Doe, J. and Smith, J. (2026).",
    "networkformat: Convert Machine Learning Tree Models to Network Edgelists.",
    "R package version 0.0.0.9000.",
    "https://github.com/yourusername/networkformat"
  )
)
